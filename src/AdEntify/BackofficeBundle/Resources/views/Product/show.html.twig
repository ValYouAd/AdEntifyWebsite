{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Product</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Tagscount</th>
                <td>{{ entity.tagsCount }}</td>
            </tr>
            <tr>
                <th>Purchaseurl</th>
                <td>{{ entity.purchaseUrl }}</td>
            </tr>
            <tr>
                <th>Mediumurl</th>
                <td>{{ entity.mediumUrl }}</td>
            </tr>
            <tr>
                <th>Smallurl</th>
                <td>{{ entity.smallUrl }}</td>
            </tr>
            <tr>
                <th>Originalurl</th>
                <td>{{ entity.originalUrl }}</td>
            </tr>
            <tr>
                <th>Minage</th>
                <td>{{ entity.minAge }}</td>
            </tr>
            <tr>
                <th>Legalnotice</th>
                <td>{{ entity.legalNotice }}</td>
            </tr>
            <tr>
                <th>Costpertag</th>
                <td>{{ entity.costPerTag }}</td>
            </tr>
            <tr>
                <th>Adentifyfees</th>
                <td>{{ entity.adentifyFees }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('product') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('product_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
